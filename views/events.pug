extends layout

block body-pack

	
	// Page Title Section
	section.inner_cover.parallax-window(data-parallax='scroll', data-image-src='assets/img/bg/bg-img.png')
		.overlay_dark
		.container
			.row.justify-content-center.align-items-center
				.col-12
					.inner_cover_content
						h3
	// Page Title Section End

	// Events Section
	section.pt100.pb100
		.container
			.event_box
				img(src='assets/img/events/event1.png', alt='event')
				.event_info
					.event_title
					.speakers
						strong Timings 
						span 2000 to 2000
					.event_date
				.event_word
					.row.justify-content-center
						.col-md-6.col-12
							h5 What is a hackthon?
						.col-md-6.col-12
			.event_box
				img(src='assets/img/events/event2.png', alt='event')
				.event_info
					.event_title
					.speakers
						strong Duration 
						span 3 Hours
					.event_date
				.event_word
					.row.justify-content-center
						.col-md-6.col-12
						.col-md-6.col-12
			.event_box
				img(src='assets/img/events/event3.png', alt='event')
				.event_info
					h5.event_title
					.event_date
				.event_word
					.row.justify-content-center
						.col-md-6.col-12
						.col-md-6.col-12
	// Events Section End