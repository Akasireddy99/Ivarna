extends layout

block body-pack
	
	// Page Title Section
	section.inner_cover.parallax-window(data-parallax='scroll', data-image-src='assets/img/bg/bg-img.png')
		.overlay_dark
		.container
			.row.justify-content-center.align-items-center
				.col-12
					.inner_cover_content
						h3
	// Page Title Section End

	// Contact Section
	section.pt100.pb100
		.container
			.col-md-8.col-12
				.contact_info
					h5
					p
					ul.social_list
						li
							a(href='#')
								i.ion-social-facebook
						li
							a(href='#')
								i.ion-social-twitter
						li
							a(href='#')
								i.ion-social-instagram
					ul.icon_list.pt50
						li
							i.ion-location
							span Aziz Nagar, Hyderabad, Telangana 500075
						li
							i.ion-ios-telephone
							span
						li
							i.ion-email-unread
							span
						li
							i.ion-planet
							span
	// Contact Section End
